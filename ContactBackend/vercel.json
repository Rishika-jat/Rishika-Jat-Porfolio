{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/src",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/ContactBackend/",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/src/index.html"
      }
    ]
  }
  